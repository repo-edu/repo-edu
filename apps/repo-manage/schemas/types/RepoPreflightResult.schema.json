{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RepoPreflightResult",
  "type": "object",
  "properties": {
    "collisions": {
      "type": "array",
      "items": {
        "$ref": "./RepoCollision.schema.json"
      }
    },
    "ready_count": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false,
  "required": [
    "collisions",
    "ready_count"
  ],
  "description": "Preflight result for repo operations"
}
