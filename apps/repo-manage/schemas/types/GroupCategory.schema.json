{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "GroupCategory",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "role": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "self_signup": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "course_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "group_limit": {
      "anyOf": [
        {
          "type": "integer",
          "minimum": 0,
          "x-rust": {
            "type": "u32"
          }
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "role",
    "self_signup",
    "course_id",
    "group_limit"
  ],
  "description": "Group category (group set) for frontend binding"
}
