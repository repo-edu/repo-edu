{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CachedLmsGroup",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "lms_member_ids": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "resolved_member_ids": {
      "type": "array",
      "items": {
        "$ref": "./StudentId.schema.json"
      }
    },
    "unresolved_count": {
      "type": "integer",
      "minimum": 0
    },
    "needs_reresolution": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "lms_member_ids",
    "resolved_member_ids",
    "unresolved_count",
    "needs_reresolution"
  ],
  "description": "Cached LMS group with resolved and unresolved member tracking"
}
