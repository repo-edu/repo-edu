{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "GitVerifyResult",
  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "message": {
      "type": "string"
    },
    "username": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "success",
    "message"
  ],
  "description": "Result of verifying a git connection"
}
