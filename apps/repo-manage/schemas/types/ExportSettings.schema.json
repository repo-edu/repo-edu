    },
    "output_xlsx": {
      "type": "boolean"
    },
    "output_yaml": {
      "type": "boolean"
    },
    "csv_file": {
      "type": "string"
    },
    "xlsx_file": {
      "type": "string"
    },
    "yaml_file": {
      "type": "string"
    },
    "member_option": {
