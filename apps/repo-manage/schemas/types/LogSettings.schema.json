{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "LogSettings",
  "type": "object",
  "properties": {
    "debug": {
      "type": "boolean"
    },
    "error": {
      "type": "boolean"
    },
    "info": {
      "type": "boolean"
    },
    "warning": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "debug",
    "error",
    "info",
    "warning"
  ],
  "description": "Logging settings (stored in AppSettings)"
}
