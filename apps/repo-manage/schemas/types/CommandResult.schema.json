{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CommandResult",
  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "message": {
      "type": "string"
    },
    "details": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "success",
    "message",
    "details"
  ]
}
