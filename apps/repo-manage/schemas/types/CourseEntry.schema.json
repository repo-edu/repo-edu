{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CourseEntry",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name"
  ],
  "description": "A course entry with ID and optional name (populated after verification)"
}
