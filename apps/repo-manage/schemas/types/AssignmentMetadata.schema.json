{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AssignmentMetadata",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "./AssignmentId.schema.json"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "assignment_type": {
      "$ref": "./AssignmentType.schema.json"
    },
    "group_set_cache_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "assignment_type"
  ],
  "description": "Assignment metadata without groups"
}
