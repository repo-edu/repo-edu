{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RosterSource",
  "type": "object",
  "properties": {
    "kind": {
      "type": "string",
      "enum": [
        "lms",
        "file",
        "manual"
      ]
    },
    "lms_type": {
      "$ref": "./LmsType.schema.json"
    },
    "base_url": {
      "type": "string"
    },
    "fetched_at": {
      "type": "string",
      "format": "date-time",
      "x-rust": {
        "type": "chrono::DateTime<chrono::Utc>"
      }
    },
    "file_name": {
      "type": "string"
    },
    "imported_at": {
      "type": "string",
      "format": "date-time",
      "x-rust": {
        "type": "chrono::DateTime<chrono::Utc>"
      }
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "x-rust": {
        "type": "chrono::DateTime<chrono::Utc>"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "kind"
  ],
  "description": "Tracks how the roster was sourced for display"
}
