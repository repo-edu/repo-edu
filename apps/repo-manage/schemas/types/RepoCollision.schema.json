{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "RepoCollision.schema.json",
  "title": "RepoCollision",
  "type": "object",
  "properties": {
    "group_id": {
      "type": "string",
      "format": "uuid"
    },
    "group_name": {
      "type": "string"
    },
    "repo_name": {
      "type": "string"
    },
    "kind": {
      "$ref": "./RepoCollisionKind.schema.json"
    }
  },
  "additionalProperties": false,
  "required": ["group_id", "group_name", "repo_name", "kind"],
  "description": "Repo collision detail"
}
