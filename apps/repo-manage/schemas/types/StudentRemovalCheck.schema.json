{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "StudentRemovalCheck",
  "type": "object",
  "properties": {
    "student_id": {
      "$ref": "./StudentId.schema.json"
    },
    "student_name": {
      "type": "string"
    },
    "affected_groups": {
      "type": "array",
      "items": {
        "$ref": "./AffectedGroup.schema.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "student_id",
    "student_name",
    "affected_groups"
  ],
  "description": "Check result for removing a student"
}
