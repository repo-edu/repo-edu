{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "RepoManage",
    "version": "0.1.0",
    "identifier": "com.dvbeek.--rc",
    "build": {
        "beforeDevCommand": {
            "script": "pnpm dev",
            "cwd": ".."
        },
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": {
            "script": "pnpm run build",
            "cwd": ".."
        },
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "RepoManage",
                "width": 700,
                "height": 900,
                "visible": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
