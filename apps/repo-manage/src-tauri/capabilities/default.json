{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-message",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "dialog:allow-open",
    "core:window:allow-close",
    "core:window:allow-destroy"
  ]
}
